{
  "name": "@webformula/pax-components",
  "version": "0.0.1-beta",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "nodemon --watch src --watch src-docs -e js,css --exec npm start",
    "start": "npm run build && http-server dist-docs/",
    "build": "npm run build:components && npm run build:docs && npm run build:copy-files",
    "build:components": "webpack --config webpack.config.js",
    "build:docs": "node src-docs/index.js",
    "build:copy-files": "cp -r src-docs/public/* dist-docs/ && cp -r dist/* dist-docs",
    "serve": "http-server dist/",
    "temp": "node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webformula/pax-components.git"
  },
  "author": "Ben Rubin (B-3PO)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webformula/pax-components/issues"
  },
  "homepage": "https://github.com/webformula/pax-components#readme",
  "devDependencies": {
    "css-loader": "^2.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@webformula/pax-core": "file:../pax-core",
    "common-tags": "^1.8.0",
    "glob": "^7.1.3",
    "http-server": "^0.11.1",
    "npm-watch": "^0.6.0"
  }
}
